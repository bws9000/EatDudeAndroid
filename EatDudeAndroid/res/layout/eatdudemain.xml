<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
					 android:layout_width="fill_parent" 
					 android:layout_height="fill_parent"
					 android:fillViewport="true"
					 android:id="@+id/scrollView111">				 

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/eatdude_main"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    
<ImageView 
   android:id="@+id/eatdudeicon"
   android:src="@drawable/eatdudeicon"
   android:layout_marginTop="10dip"
   android:layout_width="200dip"
   android:layout_height="279dip"
   android:layout_gravity="center" />
   
   
   <Button android:text="Start Here" 
			  android:id="@+id/mainbutton1" 
			  android:layout_width="200dip" 
			  android:layout_height="wrap_content"
			  android:layout_gravity="center" />
			  
  
<TextView
		android:id="@+id/eatdudemaintextview"
        android:layout_width="200dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dip"
        android:layout_gravity="center"
        android:layout_marginBottom="10dip"
        android:textColor="#000"  />

</LinearLayout>
</ScrollView>